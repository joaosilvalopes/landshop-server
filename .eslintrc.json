{
    "root": true,
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": 0,
        "no-underscore-dangle": 0,
        "object-curly-newline": 0,
        "arrow-parens": 0,
        "no-console": 0,
        "max-len": [ "error", 200, 2, {
                "ignoreUrls": true,
                "ignoreComments": false,
                "ignoreRegExpLiterals": true,
                "ignoreStrings": false,
                "ignoreTemplateLiterals": false
            }
        ]
    }
}